alue={BookingPrice || val.book_price}\r\n              onChange={e => setBookingPrice(parseInt(e.target.value))}\r\n            />\r\n          </div>\r\n          <div>\r\n            <label htmlFor=\"note\" className=\"form-label\">\r\n              หมายเหตุ\r\n            </label>\r\n            <input\r\n             maxLength=\"250\"\r\n              type=\"text\"\r\n              className=\"form-control\"\r\n              id=\"note\"\r\n              value={note || val.h_note}\r\n              onChange={e => setNote(e.target.value)}\r\n            />\r\n          </div>\r\n          <div>\r\n            <label htmlFor=\"image\" className=\"form-label\">\r\n              รูปภาพ\r\n            </label>\r\n            <input\r\n              type=\"file\"\r\n              className=\"form-control\"\r\n              id=\"image\"\r\n              onChange={e => setImage(e.target.files[0])}\r\n            />\r\n          </div>\r\n          <div>\r\n            <label htmlFor=\"zoneId\" className=\"form-label\">\r\n              โซนบ้าน\r\n            </label>\r\n            <DropdownButton\r\n              id=\"zoneId\"\r\n              title={\r\n                zoneId !== ''\r\n                  ? zones.find(zone => zone.hz_id === zoneId)?.name\r\n                  : 'เลือกโซนบ้าน'\r\n              }\r\n              onSelect={e => setZoneId(parseInt(e))}\r\n            >\r\n              {zones.map((zone, idx) => (\r\n                <Dropdown.Item key={idx} eventKey={zone.hz_id}>\r\n                  {zone.name}\r\n                </Dropdown.Item>\r\n              ))}\r\n            </DropdownButton>\r\n          </div>\r\n        </div>\r\n        ))}\r\n        <button type=\"button\" className=\"btn btn-primary\" onClick={EditHouse}>\r\n          แก้ไข\r\n        </button>\r\n      </form>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default HousingEdit;","C:\\Users\\TongNPZ\\Desktop\\my-app-2.0\\src\\components\\Zoneing\\ZoneEdit\\ZoneEdit.js",["280"],[],"imp